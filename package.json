{
    "name": "premarketprice",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "cross-env ENABLE_WEBSOCKET=true tsx server.ts",
        "dev:next": "cross-env ENABLE_WEBSOCKET=true next dev --turbopack -H 127.0.0.1 -p 3000",
        "dev:no-websocket": "next dev --turbopack -H 127.0.0.1 -p 3000",
        "build": "prisma generate && next build",
        "start": "cross-env ENABLE_WEBSOCKET=true tsx server.ts",
        "start:no-websocket": "next start",
        "lint": "next lint",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:ci": "jest --ci --coverage --watchAll=false",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:migrate": "prisma migrate dev",
        "db:studio": "prisma studio",
        "db:seed": "tsx prisma/seed.ts",
        "fetch-logos": "tsx scripts/fetch-logos.ts",
        "test:performance": "node scripts/performance-test.js",
        "test:mobile": "NEXT_PUBLIC_ENABLE_MOBILE_TESTING=true npm run dev",
        "websocket:status": "curl http://localhost:3000/api/websocket",
        "websocket:start": "curl -X POST http://localhost:3000/api/websocket -H 'Content-Type: application/json' -d '{\"action\":\"start\"}'",
        "websocket:stop": "curl -X POST http://localhost:3000/api/websocket -H 'Content-Type: application/json' -d '{\"action\":\"stop\"}'"
    },
    "dependencies": {
        "@prisma/client": "^5.22.0",
        "@types/bcryptjs": "^3.0.0",
        "@types/better-sqlite3": "^7.6.13",
        "@types/jsonwebtoken": "^9.0.10",
        "bcryptjs": "^3.0.2",
        "better-sqlite3": "^12.2.0",
        "cors": "^2.8.5",
        "decimal.js": "^10.6.0",
        "express-rate-limit": "^8.0.1",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.2",
        "lucide-react": "^0.533.0",
        "next": "15.4.4",
        "next-themes": "^0.4.6",
        "prisma": "^5.22.0",
        "prom-client": "^15.1.3",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "redis": "^5.6.1",
        "simple-icons": "^15.8.0",
        "socket.io": "^4.8.1",
        "socket.io-client": "^4.8.1",
        "sqlite3": "^5.1.7"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@playwright/test": "^1.54.2",
        "@svgr/webpack": "^8.1.0",
        "@types/jest": "^30.0.0",
        "@types/node": "24.1.0",
        "@types/react": "19.1.9",
        "@types/react-dom": "^19",
        "autoprefixer": "^10",
        "cross-env": "^10.0.0",
        "eslint": "^9",
        "eslint-config-next": "15.4.4",
        "jest": "^30.0.5",
        "postcss": "^8",
        "puppeteer": "^24.15.0",
        "tailwindcss": "^3.4.0",
        "ts-jest": "^29.4.0",
        "tsx": "^4.20.3",
        "typescript": "5.8.3"
    },
    "prisma": {
        "seed": "tsx prisma/seed.ts"
    }
}